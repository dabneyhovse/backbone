{
  "name": "dabney_backbone",
  "version": "0.1",
  "description": "A website made to be modular, without users having to touch the main code. I've constructed it likke this so we only need one node server running. This is sure to be impllemented poorly but it will work so what can you do. We dont have any super secure data here so.",
  "main": "src/server/index.js",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "link-services": "npm link service-frotator service-example service-gallery",
    "clean-and-link": "rm -rf node_modules && npm install && npm run link-services",
    "build": "webpack",
    "build-watch": "webpack --watch",
    "start": "node src/server",
    "start-server": "nodemon src/server -e html,js,scss --ignore public --ignore client",
    "start-dev": "NODE_ENV='development' npm run build-watch & NODE_ENV='development' npm run start-server &",
    "deploy": "webpack git add -A",
    "pretty": "npx prettier --write \"./**/*.{js,jsx,json,css}\" --quiet",
    "prestart": "cp ./src/script/precommit.sh .git/hooks/ && chmod +x .git/hooks/precommit.sh && echo 'precommit hook copied'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint-plugin-react": "^7.31.7",
    "file-loader": "^6.2.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@sendgrid/mail": "^8.1.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.4",
    "core-js": "^3.24.1",
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "history": "^5.3.0",
    "image-data-uri": "^2.0.1",
    "mdb-react-ui-kit": "^4.2.0",
    "module-dabney-email": "file:../Modules/module-dabney-email",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "nodemon": "^3.0.1",
    "passport": "^0.6.0",
    "pg": "^8.7.3",
    "prompt": "^1.3.0",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-grid-gallery": "^0.5.5",
    "react-icons": "^4.4.0",
    "react-loading": "^2.0.3",
    "react-redux": "^8.0.2",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.3.0",
    "react-slide-routes": "^2.0.0",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sequelize": "^6.21.4",
